{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Formula1-DataAnalytics-ADF"
		},
		"ls_ProjectFormula1_ws_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0506-145247-dtgd0vh9"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pl_ingest_formula1_data')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Ingesting the formula1 data",
				"activities": [
					{
						"name": "Ingest Circuits File",
						"type": "DatabricksNotebook",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"notebookPath": "/Formula1/ingestion/1.ingest_circuits_file"
						},
						"linkedServiceName": {
							"referenceName": "ls_ProjectFormula1_ws",
							"type": "LinkedServiceReference"
						}
					}
				],
				"concurrency": 1,
				"policy": {
					"elapsedTimeMetric": {}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_ProjectFormula1_ws')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls_ProjectFormula1_ws')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureDatabricks",
				"typeProperties": {
					"domain": "https://adb-730197986473670.10.azuredatabricks.net",
					"authentication": "MSI",
					"workspaceResourceId": "/subscriptions/8ffee2bf-cf04-43c0-8b8e-f8d18d009f65/resourceGroups/AM-ProjectFormula1/providers/Microsoft.Databricks/workspaces/ProjectFormula1-ws",
					"existingClusterId": "[parameters('ls_ProjectFormula1_ws_properties_typeProperties_existingClusterId')]"
				}
			},
			"dependsOn": []
		}
	]
}